{
    "manifest_version": 2,
    "name": "chrome-gpg-agent",
    "version": "1.0",
    "default_locale": "en",
    "description": "Communicate with gpg-agent",
    "author": "Travis Beatty <travisby@gmail.com>",
    "homepage_url": "https://github.com/travisby/chrome-gpg-agent",
    "browser_action": {
        "default_popup": "popup.html" 
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": ["nativeMessaging"]
}
